<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<GeneratePackageOnBuild>true</GeneratePackageOnBuild>

	  <PackageId>DoclingSharp</PackageId>
	  <Version>8.0.0</Version>
	  <Authors>Steve Koeniger</Authors>
	  <Product>DoclingSharp</Product>
	  <Description>.NET 8 client for Docling Serve API.</Description>
	  <PackageTags>docling;doc;api;client</PackageTags>
	  <RepositoryUrl>https://github.com/stevekoeniger/DoclingSharp</RepositoryUrl>
	  <PackageLicenseExpression>MIT</PackageLicenseExpression>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http.Features" Version="5.0.17" />
    <PackageReference Include="Microsoft.Extensions.Http" Version="9.0.8" />
  </ItemGroup>

</Project>
